<h5>Create a Song</h5>
<div class="row">
	<div>
		<%= form_for(@song, remote: true) do |f| %>
			<%= render 'shared/error_messages', object: f.object %>

			<%= f.label :name %>
			<%= f.text_field :name %>

			<%= f.label :album %>
			<%= f.text_field :album %>

			<%= f.label :artist %>
			<%= f.text_field :artist %>
			<br />
			<%= f.submit "Create Song", class: "btn btn-medium btn-primary" %>
		<% end %>
	</div>
</div>